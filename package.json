{
  "name": "tpl-template-injection",
  "displayName": "tpl Tagged Template Injection",
  "description": "Syntax highlighting injection for tpl.ts/css/json/vue tagged template literals in TS/JS.",
  "version": "0.0.4",
  "publisher": "vona-js",
  "author": {
    "name": "vona-js"
  },
  "license": "MIT",
  "icon": "images/icon.png",
  "homepage": "https://github.com/xuasir/tagged-template-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/xuasir/tagged-template-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/xuasir/tagged-template-plugin/issues"
  },
  "scripts": {
    "debug": "mkdir -p .vscode-dev && code \"$PWD\" --extensionDevelopmentPath=\"$PWD\" --user-data-dir \"$PWD/.vscode-dev\" --new-window",
    "package": "mkdir -p dist && pnpm dlx @vscode/vsce package --out dist/tpl-template-injection.vsix",
    "publish:patch": "pnpm dlx @vscode/vsce publish patch",
    "publish:minor": "pnpm dlx @vscode/vsce publish minor",
    "publish:major": "pnpm dlx @vscode/vsce publish major"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "extensionDependencies": [
    "Vue.volar"
  ],
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "scopeName": "tpl.injection",
        "path": "./syntaxes/tpl-injection.tmLanguage.json",
        "injectTo": [
          "source.ts",
          "source.tsx",
          "source.js",
          "source.js.jsx",
          "source.jsx"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.html": "html",
          "meta.embedded.block.css": "css",
          "meta.embedded.block.scss": "scss",
          "meta.embedded.block.json": "json",
          "meta.embedded.block.vue": "vue",
          "meta.embedded.block.typescript": "typescript",
          "meta.embedded.block.typescript.dts": "typescript",
          "meta.embedded.block.typescriptreact": "typescriptreact",
          "meta.embedded.block.javascript": "javascript",
          "meta.embedded.block.javascriptreact": "javascriptreact"
        },
        "tokenTypes": {
          "meta.embedded.block.html": "other",
          "meta.embedded.block.css": "other",
          "meta.embedded.block.scss": "other",
          "meta.embedded.block.json": "other",
          "meta.embedded.block.vue": "other",
          "meta.embedded.block.typescript": "other",
          "meta.embedded.block.typescript.dts": "other",
          "meta.embedded.block.typescriptreact": "other",
          "meta.embedded.block.javascript": "other",
          "meta.embedded.block.javascriptreact": "other"
        }
      }
    ]
  }
}
